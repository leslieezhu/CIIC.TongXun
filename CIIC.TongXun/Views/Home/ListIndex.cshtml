@using ZJ.App.Entity;
@model CIIC.TongXun.ChannlModel
<table width="800" border="0" align="center" cellpadding="0" cellspacing="0">
    <tr>
        <td><img src="images/tx_23.jpg" width="800" height="28" /></td>
    </tr>
</table>
<table class="content" width="800" border="0" align="center" cellpadding="0" cellspacing="0">
    <tr>
        <td width="95" valign="top" background="images/tx_24.jpg"><img src="images/tx_24.jpg" width="95" height="520" /></td>
        <td valign="top">
            <table width="100%" border="0" cellspacing="0" cellpadding="0">
                <tr>
                    <td><img src="images/@ViewBag.ListIndexImg" width="140" height="37" /></td>
                    <td align="right" valign="bottom">&nbsp;</td>
                </tr>
            </table>
            <table width="92%" border="0" align="center" cellpadding="0" cellspacing="0">
                <tr>
                    <td>&nbsp;</td>
                    <td>&nbsp;</td>
                </tr>
                @foreach (var item in Model.articles)
                {
                    <tr>
                        <td width="25" height="24"><img src="images/tx_30.jpg" width="15" height="12" /></td>
                        <td><a href="@item.Href">@item.ArticleTitle</td>
                    </tr>
                }
                <tr>
                    <td>&nbsp;</td>
                    <td>&nbsp;</td>
                </tr>
            </table>
        </td>
        <td width="95" valign="top" background="images/tx_27.jpg"><img src="images/tx_27.jpg" width="95" height="520" /></td>
    </tr>
</table>
<table width="800" border="0" align="center" cellpadding="0" cellspacing="0">
    <tr>
        <td><img src="images/tx_37.jpg" width="800" height="35" /></td>
    </tr>
</table>
